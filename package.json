{
  "name": "be5-react",
  "version": "1.0.0",
  "description": "",
  "main": "gulpfile.js",
  "scripts": {
    "start": "ws -d dist",
    "dev": "webpack-dev-server --progress --profile --colors",
    "build": "webpack --config webpack.production.config.js --progress --profile --colors --env.build --display-modules",
    "build-min": "webpack --config webpack.production.config.js --progress --profile --colors --env.build --env.min --display-modules",
    "test": "jest",
    "watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage",
    "coveralls": "npm run-script coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "beanexplorer-react": "git+https://git@github.com/DevelopmentOnTheEdge/beanexplorer-react.git#v0.1.7"
  },
  "devDependencies": {
    "prop-types": "^15.5.10",
    "node-sass": "^4.3.0",
    "sass-loader": "^6.0.2",
    "babel-core": "^6.23.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-project-relative-import": "^2.0.1",
    "babel-runtime": "^6.22.0",
    "coveralls": "^2.13.1",
    "css-loader": "0.26.1",
    "datatables": "^1.10.13",
    "enzyme": "^2.8.2",
    "extract-text-webpack-plugin": "^v2.0.0-rc.1",
    "file-loader": "^0.10.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-util": "^3.0.8",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^20.0.4",
    "jqueryui": "^1.11.1",
    "json-pointer": "^0.6.0",
    "json-server": "^0.10.1",
    "local-web-server": "^1.2.7",
    "number-format.js": "^1.1.11",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "style-loader": "0.13.1",
    "tether": "^1.4.0",
    "underscore": "^1.8.3",
    "url-loader": "0.5.7",
    "webpack": "^2.6.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.4.5"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/DevelopmentOnTheEdge/be5-react.git"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|sass|png|jpg)$": "<rootDir>/test/fileTransformer.js"
    },
    "roots": [
      "./test/",
      "./src/scripts"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared",
      "./src/scripts"
    ]
  }
}
