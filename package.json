{
  "name": "be5-react",
  "version": "0.2.110",
  "description": "React client UI for be5 server api",
  "main": "dist/lib/be5-react.js",
  "scripts": {
    "dev": "ws -d dist/uncompressed",
    "start": "webpack-dashboard -- webpack-dev-server --host 127.0.0.1 --progress --profile --colors",
    "build": "webpack --config webpack.production.config.js --progress --profile --colors --env.build",
    "build-min": "webpack --config webpack.production.config.js --progress --profile --colors --env.build --env.min",
    "test": "jest",
    "watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage",
    "coveralls": "npm run-script coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "up": "npm version patch && npm publish",
    "lib": "rollup -c rollup.config.js"
  },
  "dependencies": {
    "beanexplorer-react": "^1.0.25",
    "bootstrap": "4.1.3",
    "classnames": "2.2.5",
    "datatables.net": "1.10.16",
    "formdata-polyfill": "^3.0.13",
    "jquery": "3.2.1",
    "json-pointer": "0.6.0",
    "number-format.js": "1.1.11",
    "prop-types": "15.6.1",
    "react": "15.6.1",
    "react-ace": "5.9.0",
    "react-codemirror": "1.0.0",
    "react-dom": "15.6.1",
    "react-redux": "^5.0.7",
    "react-s-alert": "1.3.1",
    "react-split-pane": "0.1.77",
    "react-transition-group": "^2.5.3",
    "reactstrap": "5.0.0-beta",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "tether": "1.4.0",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "2.13.1",
    "css-loader": "1.0.1",
    "cssnano": "4.1.7",
    "enzyme": "2.8.2",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.10.0",
    "html-webpack-plugin": "2.26.0",
    "jest": "22.4.2",
    "json-server": "0.10.1",
    "local-web-server": "1.2.7",
    "node-sass": "4.11.0",
    "optimize-css-assets-webpack-plugin": "2.0.0",
    "postcss-loader": "1.2.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "15.5.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "15.6.1",
    "rollup": "0.52.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-image-base64": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "sass-loader": "6.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "3.3.0",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-cleanup-plugin": "0.5.1",
    "webpack-dashboard": "0.4.0",
    "webpack-dev-server": "2.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DevelopmentOnTheEdge/be5-react.git"
  },
  "jest": {
    "testURL": "http://localhost/",
    "verbose": true,
    "globals": {
      "NODE_ENV": "test"
    },
    "setupFiles": [
      "<rootDir>/test/jestGlobals.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|png|jpg)$": "<rootDir>/test/fileTransformer.js"
    },
    "roots": [
      "./test/",
      "./src/scripts"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/scripts"
    ]
  }
}
